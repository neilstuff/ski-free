{
    "name": "ski-free",
    "version": "1.0.0",
    "description": "Ski Free",
    "email": "thesourorange@brittliff.org",
    "main": "main.js",
    "scripts": {
        "pack": "electron-builder --dir",
        "dist": "electron-builder --publish never",
        "start": "electron ."
    },
    "build": {
        "win": {
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "x64",
                        "ia32"
                    ]
                }
            ]
        },
        "linux": {
            "target": [
                "snap"
            ],
            "category": "Game"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/thesourorange/neilstuff/ski-free.git"
    },
    "keywords": [
        "Electron",
        "game"
    ],
    "author": "thesouorange <thesouroange@brittliff.org>",
    "license": "CC0-1.0",
    "dependencies": {
        "jquery": "^3.6.0",
        "mime": "^3.0.0"
    },
    "devDependencies": {
        "electron": "^19.0.6",
        "electron-builder": "^22.11.7"
    },
    "bugs": {
        "url": "https://github.com/thesourorange/ski-free/issues"
    },
    "homepage": "https://github.com/thesourorange/ski-free#readme"
}
